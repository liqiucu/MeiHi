@model IEnumerable<MeiHi.Admin.Models.Service.ServiceTypeModel>

@{
    ViewBag.Title = "ManageServiceType";
}

<h2>服务类型管理</h2>

<p>
    @Html.ActionLink("创建服务类型", "CreateServiceType")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ServiceTypeId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ServiceTypeName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ServiceTypeId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ServiceTypeName)
        </td>
        <td>
            @Html.ActionLink("编辑服务类型", "EditServiceType", new { serviceTypeId = item.ServiceTypeId }) |
            @Html.ActionLink("删除服务类型", "DeleteServiceType", new { serviceTypeId = item.ServiceTypeId }, new { onclick = "return confirm('确定要下架吗')" })
        </td>
    </tr>
}

</table>
<div>
    @Html.ActionLink("返回服务类型管理", "ManageServiceType")
</div>